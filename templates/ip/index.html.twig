{% extends 'base.html.twig' %}

{% block title %}ip{% endblock %}

{% block body %}
<main class="container">

<div class="input-group pt-4 pb-2">
  <div class="input-group-prepend">
    <button class="btn btn-outline-secondary dropdown-toggle" type="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">label </button>
    <div class="dropdown-menu">
      <a class="dropdown-item" href="#">Action</a>
      <a class="dropdown-item" href="#">Another action</a>
      <a class="dropdown-item" href="#">Something else here</a>
      <div role="separator" class="dropdown-divider"></div>
      <a class="dropdown-item" href="#">Separated link</a>
    </div>
  </div>
  <input id="search" type="text" class="form-control" aria-label="Text input with dropdown button">
</div>

<table class="table table-hover">
<thead class="thead-light">
<tr>
<th scope="col">label</th>
<th scope="col">ip</th>
<th scope="col">sid</th>
<th scope="col">sname</th>
<th scope="col">zname</th>
</tr>
</thead>
<tbody id="tbody">
<script id="tpl" type"x-tmpl">
<% #rows %>
<tr>
<th scope="row" contenteditable="true"><% label %></th>
<td contenteditable="true"><% ip %></td>
<td contenteditable="true"><% sid %></td>
<td contenteditable="true"><% sname %></td>
<td contenteditable="true"><% zname %></td>
</tr>
<% /rows %>
</script>
</tbody>
</table>
</main>
{% endblock %}

{% block javascripts %}
	<script src="js/mustache.min.js"></script>
	<script src="js/z.js"></script>
{% endblock %}
