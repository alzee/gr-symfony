{% extends 'base.html.twig' %}

{% block title %}ip{% endblock %}

{% block main %}
<main class="container">
<div class="row pt-4 pb-2">
  <div class="col-auto">
    <button type="button" class="btn btn-outline-danger">
      export
      <span class="badge badge-light" id="count">0 / 0</span>
    </button>
  </div>
  <div class="input-group col">
    <input id="search" type="text" autofocus class="form-control" aria-label="Text input with dropdown button">
  </div>
</div>

<table class="table table-hover table-responsive-md">
<thead class="thead-light">
<tr>
<th scope="col">标签</th>
<th scope="col">IP</th>
<th scope="col">门店ID</th>
<th scope="col">门店名称</th>
<th scope="col">战区</th>
</tr>
</thead>
<tbody id="tbody">
{% for i in rows %}
<tr class="d-none">
<td contenteditable="true" class="">{{ i.label }}</td>
<td contenteditable="true" class="">{{ i.ip }}</td>
<td contenteditable="true" class="">{{ i.sid }}</td>
<td contenteditable="true" class="d-none pinyin"></td>
<td contenteditable="true" class="wantPinyin">{{ i.sname }}</td>
<td contenteditable="true" class="wantPinyin">{{ i.zname }}</td>
</tr>
{% endfor %}
</tbody>
</table>
</main>
{% endblock %}

{% block js1 %}
	<script src="/js/pinyin_dict_notone.js"></script>
	<script src="/js/pinyinUtil.js"></script>
{% endblock %}
